{
  "name": "blockDuplicates",
  "description": "Block duplicate uploads on BEFORE_UPLOAD",
  "version": "1.0.0",
  "scripts": {
    "deploy": "jf worker deploy",
    "undeploy": "jf worker rm \"blockDuplicates\"",
    "test": "jest"
  },
  "license": "ISC",
  "devDependencies": {
    "jfrog-workers": "^0.6.0",
    "@golevelup/ts-jest": "^0.4.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-jasmine2": "^29.7.0",
    "ts-jest": "^29.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "clearMocks": true,
    "maxConcurrency": 1,
    "testRegex": "\\.spec\\.ts$",
    "moduleDirectories": ["node_modules"],
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testRunner": "jest-jasmine2",
    "verbose": true
  }
}
